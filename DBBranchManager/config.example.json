{
  "projectRoot": "D:/EV/Solvency2",
  "projectSettingsFile": "project.settings.json",
  "databases": {
    "connection": {
      "server": "localhost",
      "user": "sa",
      "password": "xxxxx",
      "relocatePath": "D:/Databases",
      "relocate": true
    },
    "backups": {
      "root": "D:/Databases/Backups",
      "pattern": "^(?<dbName>\\w+)\\.\\[(?<release>[\\w.]+)]\\.(?<env>\\w+)\\.bak$"
    }
  },
  "envVariables": {
    "packagesPath": "E:/Packages",
    "scriptsPath": "E:/ScriptsSII",
    "environment": "dev",
    "dryRun": false
  },
  "beeps": {
    "start": {
      "frequency": 1000,
      "duration": 250
    },
    "success": {
      "frequency": 800,
      "duration": 250
    },
    "error": {
      "frequency": 800,
      "duration": 600,
      "pulses": 2,
      "dutyCycle": 0.75
    }
  }
}